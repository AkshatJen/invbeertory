{
  "name": "invbeertory",
  "description": "Simple beer inventory display",
  "version": "0.1.0",
  "author": "Cian Butler <butlerx@notthe.cloud>",
  "menu": [
    {
      "name": "Current Stock",
      "link": "/stock"
    },
    {
      "link": "/history/",
      "name": "History"
    },
    {
      "link": "/graphs/",
      "name": "Graphs"
    }
  ],
  "project": {},
  "dependencies": {
