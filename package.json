    "gatsby": "^2.18.11",
    "gatsby-plugin-manifest": "^2.2.33",
    "gatsby-plugin-offline": "^3.0.29",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-sass": "^2.1.26",
    "gatsby-plugin-typescript": "^2.1.23",
    "gatsby-source-google-spreadsheets": "^2.2.0",
    "node-sass": "^4.12.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-table": "^6.10.3"
  },
  "devDependencies": {
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "netlify-cli": "^2.25.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.3"
  },
  "keywords": [
    "gatsby",
    "beer"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --write src/**/*.{ts,tsx,css} *.{json,js,md}",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "gatsby clean",
    "deploy": "netlify deploy",
    "start": "gatsby serve"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --write --ext .js,.jsx,.ts,.tsx",
      "git add"
    ],
    "*.{css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/butlerx/invbeertory"
  },
  "bugs": {
    "url": "https://github.com/butlerx/invbeertory/issues"
  }
